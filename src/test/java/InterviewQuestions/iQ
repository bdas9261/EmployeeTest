 public class p{
   int a=10;
   public void test1(){
   System.out.println("test1 of parent method");
}
   public static void test2(){
   System.out.println("test2 of parent method");
}
}


public class c extends p{
   int a=20;
   public void test1(){
   System.out.println("test1 of child method");
}
   public static void test2(){
   System.out.println("test2 of child method");
}
    p p1=new c();
     syso(p1.a);
 p1.test1();
 p1.test2();
}

